<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"

    tools:context=".AddAnotherCommercialFragment">
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintTop_toTopOf="parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <AutoCompleteTextView
                android:id="@+id/autoCompleteTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:layout_gravity="center"
                android:layout_marginHorizontal="10dp"
                android:hint="google search address" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="center"
            >

            <EditText
                android:id="@+id/editText54"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:paddingLeft="10dp"
                android:hint="@string/editText54"
                android:inputType="textPersonName"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="center">

            <EditText
                android:id="@+id/editText55"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:paddingLeft="10dp"
                android:hint="@string/editText55"
                android:inputType="textPersonName"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="50dp">

            <EditText
                android:id="@+id/editText51"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:paddingLeft="10dp"
                android:cursorVisible="true"

                android:hint="@string/editText51"
                android:inputType="textPersonName"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />
        </TableRow>

        <EditText
            android:id="@+id/editText52"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginHorizontal="10dp"
            android:backgroundTint="@color/black"
            android:cursorVisible="true"
            android:hint="@string/editText52"
            android:paddingLeft="10dp"
            android:inputType="textPersonName"
            android:textColorHighlight="@color/black"
            android:textColorLink="@color/black"
            android:textCursorDrawable="@drawable/text_cursor"
            android:textSize="@dimen/size_text" />


        <TableRow

        android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="center">

            <EditText
                android:id="@+id/editText53"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:hint="@string/editText53"
                android:paddingLeft="10dp"
                android:inputType="textPersonName"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />
        </TableRow>




        <TableRow
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="center"
            >

            <EditText
                android:id="@+id/editText56"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:hint="@string/editText56"
                android:paddingLeft="10dp"
                android:inputType="textPersonName"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="center"
            >

            <EditText
                android:id="@+id/editText84"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:hint="@string/editText84"
                android:paddingLeft="10dp"
                android:inputType="textPersonName"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />
        </TableRow>


            <Spinner
                android:id="@+id/charger_type_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="start"
                android:popupBackground="@color/white"
                android:textSize="@dimen/size_text" />


        <Spinner
            android:id="@+id/charger_brand_spinner"
            style="@style/spinner_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:popupBackground="@color/background_icon2"
            android:textSize="@dimen/size_text"
            tools:visibility="gone" />


            <Spinner
                android:id="@+id/charger_model_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                android:textSize="@dimen/size_text"
                tools:visibility="gone"/>


            <Spinner
                android:id="@+id/charger_connector_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                android:textSize="@dimen/size_text"
                tools:visibility="gone"/>


        <Spinner
            android:id="@+id/electricity_board_spinner"
            style="@style/spinner_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:popupBackground="@color/white"
            android:textSize="@dimen/size_text"
            tools:visibility="gone"/>

        <Spinner
            android:id="@+id/rate_structure_spinner"
            style="@style/spinner_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:popupBackground="@color/white"
            android:textSize="@dimen/size_text"
            tools:visibility="gone"/>

        <Spinner
            android:id="@+id/power_output_spinner"
            style="@style/spinner_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:popupBackground="@color/white"
            android:textSize="@dimen/size_text"
            tools:visibility="gone"/>

        <Spinner
            android:id="@+id/voltage_spinner"
            style="@style/spinner_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:popupBackground="@color/white"
            android:textSize="@dimen/size_text"
            tools:visibility="gone"/>

        <Spinner
            android:id="@+id/amphere_spinner"
            style="@style/spinner_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:popupBackground="@color/white"
            android:textSize="@dimen/size_text"
            tools:visibility="gone"/>

        <Spinner
            android:id="@+id/socket_spinner"
            style="@style/spinner_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:popupBackground="@color/white"
            android:textSize="@dimen/size_text"
            tools:visibility="gone"/>

        <Spinner
            android:id="@+id/socket_electricity_board_spinner"
            style="@style/spinner_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:popupBackground="@color/white"
            android:textSize="@dimen/size_text"
            tools:visibility="gone"/>

        <Spinner
            android:id="@+id/socket_rate_spinner"
            style="@style/spinner_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:popupBackground="@color/white"
            android:textSize="@dimen/size_text"
            tools:visibility="gone"/>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center">

            <EditText
                android:id="@+id/socket_power_output_edit_text"
                android:inputType="numberDecimal"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:layout_marginHorizontal="10dp"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:gravity="center|left"
                android:paddingLeft="10dp"
                android:hint="socket power output (kwh)"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text"
                android:visibility="gone"/>

        </TableRow>

        <Spinner
            android:id="@+id/socket_voltage_spinner"
            style="@style/spinner_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:popupBackground="@color/white"
            android:textSize="@dimen/size_text"
            tools:visibility="gone"/>

        <Spinner
            android:id="@+id/socket_amphere_spinner"
            style="@style/spinner_style"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="10dp"
            android:gravity="center"
            android:popupBackground="@color/white"
            android:textSize="@dimen/size_text"
            tools:visibility="gone"/>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/final_power_output_text_view"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:layout_marginHorizontal="10dp"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:gravity="center|left"
                android:paddingLeft="10dp"
                android:hint="final power output (kwh)"
                android:inputType="textPersonName"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text"
                android:visibility="gone"/>

        </TableRow>


        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="20dp"></TableRow>

        <Button
            android:id="@+id/nextButton"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="120dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:background="@drawable/roundedbutton"
            android:hint="@string/button42"
            android:textSize="@dimen/size_B"
            android:textStyle="bold" />
<!--        <fragment android:id="@+id/autocomplete_fragment"-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="wrap_content"-->
<!--            android:name="com.google.android.libraries.places.widget.AutocompleteSupportFragment"-->
<!--            />-->
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="50dp" />


    </LinearLayout>
</ScrollView>



</androidx.constraintlayout.widget.ConstraintLayout>