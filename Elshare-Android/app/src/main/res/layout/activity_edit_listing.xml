<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"

    tools:context=".edit_listing_frag">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#171616"
            android:orientation="vertical">
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:orientation="horizontal"
                android:layout_marginHorizontal="@dimen/activity_horizontal_tab_elemennt">

                <Button
                    android:id="@+id/button70"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:layout_weight="50"
                    android:background="@drawable/bg_text"
                    android:gravity="center"
                    android:hint="@string/button70"
                    android:textAlignment="center"
                    android:textColorHint="@color/black"
                    android:textSize="@dimen/size_text" />

                <Button
                    android:id="@+id/button71"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="5dp"
                    android:layout_weight="50"
                    android:background="@drawable/bg_text"
                    android:gravity="center"
                    android:hint="@string/button71"
                    android:textAlignment="center"
                    android:textColorHint="@color/black"
                    android:textSize="@dimen/size_text" />

            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="10dp" >
            </TableRow>

            <Button
                android:id="@+id/button72"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:layout_width="170dp"
                android:layout_height="35dp"
                android:layout_gravity="center"
                android:background="@drawable/roundedbutton"
                android:hint="@string/button72"
                android:textSize="@dimen/size_text"
                android:textStyle="bold" />


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="5dp" >
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:layout_gravity="center"
                android:layout_marginHorizontal="@dimen/activity_horizontal_tab_elemennt">
                <TextView
                    android:id="@+id/textView63"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:background="@android:color/transparent"
                    android:hint="@string/textView63"
                    android:textColorHint="@color/blue"
                    android:textSize="@dimen/size_text"                    android:textStyle="bold"
                    android:gravity="center" />

                <TextView
                    android:id="@+id/textView64"
                    android:layout_height="50dp"
                    android:hint="@string/textView64"
                    android:layout_weight="1"
                    android:background="@android:color/transparent"
                    android:textColorHint="@color/blue"
                    android:textSize="@dimen/size_text"                    android:gravity="center"
                    android:textStyle="bold"
                    android:layout_width="match_parent"
                    />
                <TextView
                    android:id="@+id/textView65"
                    android:layout_height="50dp"
                    android:layout_width="match_parent"
                    android:hint="@string/textView65"
                    android:gravity="center"
                    android:background="@android:color/transparent"
                    android:textColorHint="@color/blue"
                    android:textSize="@dimen/size_text"                    android:textStyle="bold"
                    android:layout_weight="1"
                    />

            </TableRow>

            <TextView
                android:id="@+id/textView66"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginHorizontal="@dimen/activity_horizontal_tab_elemennt"
                android:layout_weight="1"
                android:gravity="center"
                android:hint="@string/textView66"
                android:textColorHint="@color/black"
                android:textSize="@dimen/size_text" />


            <TextView
                android:id="@+id/textView67"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginHorizontal="30dp"
                android:hint="@string/textView67"

                android:inputType="textMultiLine"
                android:paddingLeft="10dp"
                android:textColorHint="@color/black"
                android:textSize="@dimen/size_text" />

            <TextView
                android:id="@+id/textView68"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="30dp"
                android:hint="@string/textView68"
                android:paddingLeft="10dp"
                android:textColorHint="@color/black"
                android:textSize="@dimen/size_text" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginHorizontal="@dimen/activity_horizontal_tab_elemennt">

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:background="@android:color/transparent"
                    android:hint="@string/button87"
                    android:paddingLeft="10dp"
                    android:gravity="left"
                    android:textColorHint="@color/blue"
                    android:textSize="@dimen/size_text"
                    android:textStyle="bold" />

            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="15dp"

                android:orientation="horizontal"
               >

                <Spinner
                    android:id="@+id/spinner2"
                    style="@style/spinner_style"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:popupBackground="@color/white"
                    android:spinnerMode="dropdown"
                    android:textSize="@dimen/size_text" />

            </TableRow>

<!--            <TableRow-->
<!--                android:layout_width="match_parent"-->
<!--                android:layout_marginHorizontal="15dp"-->

<!--                android:layout_height="50dp">-->

<!--                <Spinner-->
<!--                    android:id="@+id/spinner3"-->
<!--                    style="@style/spinner_style"-->
<!--                    android:layout_width="match_parent"-->
<!--                    android:layout_height="match_parent"-->
<!--                    android:gravity="center"-->
<!--                    android:popupBackground="@color/white"-->
<!--                    android:textSize="@dimen/size_text" />-->

<!--            </TableRow>-->

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="15dp"
                android:layout_gravity="center"
                android:orientation="horizontal">


                <Spinner
                    android:id="@+id/spinner4"
                    style="@style/spinner_style"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="10dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:popupBackground="@color/white"
                    android:textSize="@dimen/size_text" />


                <Spinner
                    android:id="@+id/spinner5"
                    style="@style/spinner_style"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:gravity="center"

                    android:popupBackground="@color/white"
                    android:textSize="@dimen/size_text" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_marginHorizontal="15dp"

                android:layout_height="50dp">

                <Spinner
                    android:id="@+id/spinner6"
                    style="@style/spinner_style"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"

                    android:popupBackground="@color/white"
                    android:textSize="@dimen/size_text" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="15dp"

                >

                <Spinner
                    android:id="@+id/spinner7"
                    style="@style/spinner_style"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:popupBackground="@color/white"
                    android:textSize="@dimen/size_text" />

            </TableRow>


            <EditText
                android:id="@+id/editText64"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="15dp"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:hint="@string/editText64"
                android:inputType="textPersonName"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />

            <EditText
                android:id="@+id/editText65"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="15dp"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:hint="@string/editText65"
                android:inputType="textPersonName"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="20dp"></TableRow>


            <Button
                android:id="@+id/button102"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:layout_width="110dp"
                android:layout_height="35dp"
                android:layout_gravity="center"
                android:layout_marginHorizontal="15dp"
                android:background="@drawable/roundedbutton"

                android:hint="@string/button102"
                android:textSize="@dimen/size_B"
                android:textStyle="bold" />


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50dp">
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50dp">
            </TableRow>







        </LinearLayout>
    </ScrollView>

</FrameLayout>