<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".add_another_public_g">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center">

                <AutoCompleteTextView
                    android:id="@+id/autoCompleteTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:layout_gravity="center"
                    android:layout_marginHorizontal="10dp"
                    android:hint="google search address" />

            </TableRow>



            <EditText
                android:id="@+id/editText60"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:layout_weight="1"
                android:background="?attr/editTextBackground"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:paddingLeft="10dp"
                android:hint="@string/editText61"
                android:inputType="textPersonName"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />

            <EditText
                android:id="@+id/editText62"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:hint="@string/editText62"
                android:inputType="textPersonName"
                android:paddingLeft="10dp"

                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text"/>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <EditText
                    android:id="@+id/editText57"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginHorizontal="10dp"
                    android:paddingLeft="10dp"

                    android:backgroundTint="@color/black"
                    android:cursorVisible="true"
                    android:hint="@string/editText57"
                    android:inputType="textPersonName"
                    android:textColorHighlight="@color/black"
                    android:textColorLink="@color/black"
                    android:textCursorDrawable="@drawable/text_cursor"
                    android:textSize="@dimen/size_text" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <EditText
                    android:id="@+id/editText58"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:paddingLeft="10dp"

                    android:layout_marginHorizontal="10dp"
                    android:layout_weight="1"
                    android:backgroundTint="@color/black"
                    android:cursorVisible="true"
                    android:hint="@string/editText58"
                    android:inputType="textPersonName"
                    android:textColorHighlight="@color/black"
                    android:textColorLink="@color/black"
                    android:textCursorDrawable="@drawable/text_cursor"
                    android:textSize="@dimen/size_text"/>
            </TableRow>

            <EditText
                android:id="@+id/editText59"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:layout_weight="1"
                android:backgroundTint="@color/black"
                android:paddingLeft="10dp"

                android:cursorVisible="true"
                android:hint="@string/editText59"
                android:inputType="textPersonName"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />



            <EditText
                android:id="@+id/editText63"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:layout_weight="1"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:hint="@string/editText63"
                android:inputType="textPersonName"
                android:paddingLeft="10dp"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />

            <EditText
                android:id="@+id/editText85"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:layout_weight="1"
                android:backgroundTint="@color/black"
                android:cursorVisible="true"
                android:hint="@string/editText85"
                android:inputType="textPersonName"
                android:paddingLeft="10dp"
                android:textColorHighlight="@color/black"
                android:textColorLink="@color/black"
                android:textCursorDrawable="@drawable/text_cursor"
                android:textSize="@dimen/size_text" />


            <Spinner
                android:id="@+id/charger_type_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                android:textSize="@dimen/size_text" />


            <Spinner
                android:id="@+id/charger_brand_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                android:textSize="@dimen/size_text"
                tools:visibility="gone" />

            <Spinner
                android:id="@+id/charger_model_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                android:textSize="@dimen/size_text"
                tools:visibility="gone" />

            <Spinner
                android:id="@+id/charger_connector_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                tools:visibility="gone" />


            <Spinner
                android:id="@+id/electricity_board_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                android:textSize="@dimen/size_text"
                tools:visibility="gone" />

            <Spinner
                android:id="@+id/rate_structure_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                tools:visibility="gone" />

            <Spinner
                android:id="@+id/power_output_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                tools:visibility="gone" />

            <Spinner
                android:id="@+id/voltage_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                tools:visibility="gone" />

            <Spinner
                android:id="@+id/amphere_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                tools:visibility="gone" />

            <Spinner
                android:id="@+id/socket_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                tools:visibility="gone" />

            <Spinner
                android:id="@+id/socket_rate_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                tools:visibility="gone" />

            <Spinner
                android:id="@+id/socket_electricity_board_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                tools:visibility="gone" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center">

                <EditText
                    android:id="@+id/socket_power_output_edit_text"
                    android:inputType="numberDecimal"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:layout_marginHorizontal="10dp"
                    android:backgroundTint="@color/black"
                    android:cursorVisible="true"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:hint="socket power output (kwh)"
                    android:textCursorDrawable="@drawable/text_cursor"
                    android:textSize="@dimen/size_text"
                    android:visibility="gone"/>

            </TableRow>

            <Spinner
                android:id="@+id/socket_voltage_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                android:textSize="@dimen/size_text"
                tools:visibility="gone"/>

            <Spinner
                android:id="@+id/socket_amphere_spinner"
                style="@style/spinner_style"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="10dp"
                android:gravity="center"
                android:popupBackground="@color/white"
                android:textSize="@dimen/size_text"
                tools:visibility="gone"/>


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center">

                <TextView
                    android:id="@+id/final_power_output_text_view"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:layout_marginHorizontal="10dp"
                    android:backgroundTint="@color/black"
                    android:cursorVisible="true"
                    android:gravity="center|left"
                    android:paddingLeft="10dp"
                    android:hint="final power output (kwh)"
                    android:inputType="textPersonName"
                    android:textCursorDrawable="@drawable/text_cursor"
                    android:textSize="@dimen/size_text"
                    android:visibility="gone"/>

            </TableRow>


            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="20dp"></TableRow>

            <Button
                android:id="@+id/nextButton"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:layout_width="120dp"
                android:layout_height="35dp"
                android:layout_gravity="center"
                android:background="@drawable/roundedbutton"
                android:hint="@string/button88"
                android:textSize="@dimen/size_B"
                android:textStyle="bold" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50dp">


            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50dp" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50dp" />


</LinearLayout>
    </ScrollView>
</FrameLayout>